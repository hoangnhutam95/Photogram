<!DOCTYPE html>
<html>
	<head>
		<title>Photogram</title>
		<%= render "layouts/rails_default" %>
		<%= render "layouts/shim" %>
	</head>
	<body>
		<%= render 'layouts/header' %>
		<% flash.each do |message_type, message| %>
			<div class="alert alert-<%= message_type %>"><%= message %></div>
		<% end %>
		<%= yield %>
	</body>
</html>